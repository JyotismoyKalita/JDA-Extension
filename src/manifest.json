{
  "manifest_version": 3,
  "name": "JDA - Jyotismoy's Download Accelerator",
  "version": "0.1.0",
  "description": "Intercepts and accelerates downloads using JDA.",
  "permissions": [
    "downloads",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}